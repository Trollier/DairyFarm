@model IEnumerable<DairyFarm.Core.DAL.Cattle>

@{
    ViewBag.Title = "CattleInQuarantine";
}

<h2>Bete en Quarantine</h2>

<table class="table liste">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CodeCattle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateBirth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Herd.CattleType.Label)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Herd.Label)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MalParent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FemaleParent)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CodeCattle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateBirth)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Herd.CattleType.Label)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Herd.Label)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MalParent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FemaleParent)
            </td>
        </tr>
    }

</table>
